<?xml version="1.0" encoding="UTF-8"?><connectorMetaData path="jdbc">
	<name>Database Reader</name>
	<author>Mirth Corporation</author>
	<pluginVersion>@mirthversion</pluginVersion>
	<mirthVersion>3.0.0</mirthVersion>
	<url>http://www.mirthcorp.com</url>
	<description>This connector allows Mirth to poll any supported JDBC-compatabile database for data. Rows are returned as XML and JavaScript can be used for advanced logic.</description>
	<clientClassName>com.mirth.connect.connectors.jdbc.DatabaseReader</clientClassName>
	<serverClassName>com.mirth.connect.connectors.jdbc.DatabaseReceiver</serverClassName>
	<sharedClassName>com.mirth.connect.connectors.jdbc.DatabaseReceiverProperties</sharedClassName>
	<serviceClassName>com.mirth.connect.connectors.jdbc.DatabaseConnectorService</serviceClassName>
	<library path="jdbc-client.jar" type="CLIENT"/>
	<library path="jdbc-shared.jar" type="SHARED"/>
	<library path="jdbc-server.jar" type="SERVER"/>
	<protocol>jdbc</protocol>
	<type>SOURCE</type>
</connectorMetaData>
