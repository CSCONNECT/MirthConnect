<?xml version="1.0" encoding="UTF-8"?><launch4jConfig>
	<dontWrapJar>false</dontWrapJar>
	<headerType>gui</headerType>

	<jar>..\server\setup\mirth-manager-launcher.jar</jar>
	<outfile>..\server\setup\Mirth_Manager.exe</outfile>
	<icon>..\server\setup\public_html\images\mirth_128_ico.ico</icon>

	<errTitle>Mirth_Manager</errTitle>
	<chdir>.</chdir>
	<priority>normal</priority>
	<customProcName>true</customProcName>
	
	<jre>
		<minVersion>1.6.0</minVersion>
		<initialHeapSize>256</initialHeapSize>
		<maxHeapSize>512</maxHeapSize>
		<opt>-Dfile.encoding=UTF8</opt>
		<opt>-Djava.library.path=manager-lib/</opt>
	</jre>

	<singleInstance>
		<mutexName>Mirth_Manager</mutexName>
	</singleInstance>
	<versionInfo>
		<fileVersion>3.0.0.2</fileVersion>
		<txtFileVersion>Mirth_Manager 3.0.0.2</txtFileVersion>
		<fileDescription>Mirth_Manager</fileDescription>
		<copyright>(C) 2013 Mirth Corporation</copyright>
		<productVersion>3.0.0.2</productVersion>
		<txtProductVersion>Mirth_Manager 3.0.0.2</txtProductVersion>
		<productName>Mirth_Manager</productName>
		<companyName>Mirth Corporation</companyName>
		<internalName>Mirth_Manager</internalName>
		<originalFilename>Mirth_Manager.exe</originalFilename>
	</versionInfo>
</launch4jConfig>
